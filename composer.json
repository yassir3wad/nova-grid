{
    "name": "jobcerto/nova-grid",
    "description": "Add coluns to create and Update resources",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require":
    {
        "php": ">=7.1.0"
    },
    "autoload":
    {
        "psr-4":
        {
            "Jobcerto\\NovaGrid\\": "src/"
        }
    },
    "extra":
    {
        "laravel":
        {
            "providers": [
                "Jobcerto\\NovaGrid\\ToolServiceProvider"
            ]
        }
    },
    "config":
    {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
